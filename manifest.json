{
  "manifest_version": 3,
  "name": "Iframe Color Changer",
  "version": "1.0",
  "description": "Inject an iframe to change page background color.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  },
  "host_permissions": [
  "<all_urls>"
]
,
  "background": {
  },
  "content_scripts": [],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"]
    }
  ]
}
